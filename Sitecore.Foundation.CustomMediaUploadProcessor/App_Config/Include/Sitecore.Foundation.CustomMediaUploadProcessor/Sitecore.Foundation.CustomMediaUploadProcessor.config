<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<processors>
			<uiUpload>
				<!-- Run after Sitecore’s own processors so CreatedItem is available -->
				<processor mode="on" patch:after="processor[@type='Sitecore.Pipelines.Upload.Done, Sitecore.Kernel']" type="Sitecore.Foundation.CustomMediaUploadProcessor.AfterUploadProcessor, Sitecore.Foundation.CustomMediaUploadProcessor" />
			</uiUpload>
		</processors>
	</sitecore>
</configuration>
